#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pywal==0.3.10','console_scripts','wal'
__requires__ = 'pywal==0.3.10'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pywal==0.3.10', 'console_scripts', 'wal')()
    )
