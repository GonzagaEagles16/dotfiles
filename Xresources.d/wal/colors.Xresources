! Rofi colors.
rofi.color-window: #0A0A13, #0A0A13, #D35B9D
rofi.color-normal: #0A0A13, #EFEFF1, #0A0A13, #D35B9D, #0A0A13
rofi.color-active: #0A0A13, #EFEFF1, #0A0A13, #D35B9D, #0A0A13
rofi.color-urgent: #0A0A13, #A85892, #0A0A13, #A85892, #EFEFF1

! Xclock colors.
XClock*foreground: #EFEFF1
XClock*background: #0A0A13
XClock*majorColor:  rgba:ef/ef/f1/ff
XClock*minorColor:  rgba:ef/ef/f1/ff
XClock*hourColor:   rgba:ef/ef/f1/ff
XClock*minuteColor: rgba:ef/ef/f1/ff
XClock*secondColor: rgba:ef/ef/f1/ff

! Set depth to make transparency work.
URxvt*depth: 32
