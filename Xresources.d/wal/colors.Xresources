! Black color that will not be affected by bold highlighting.
*.color66: #212B3C
*color66:  #212B3C

! Rofi colors.
rofi.color-window: #212B3C, #212B3C, #9EB8CC
rofi.color-normal: #212B3C, #C1D3E5, #212B3C, #9EB8CC, #212B3C
rofi.color-active: #212B3C, #C1D3E5, #212B3C, #9EB8CC, #212B3C
rofi.color-urgent: #212B3C, #89A6B9, #212B3C, #89A6B9, #C1D3E5

! Xclock colors.
XClock*majorColor:  rgba:c1/d3/e5/ff
XClock*minorColor:  rgba:c1/d3/e5/ff
XClock*hourColor:   rgba:c1/d3/e5/ff
XClock*minuteColor: rgba:c1/d3/e5/ff
XClock*secondColor: rgba:c1/d3/e5/ff

! Set depth to make transparency work.
URxvt*depth: 32
